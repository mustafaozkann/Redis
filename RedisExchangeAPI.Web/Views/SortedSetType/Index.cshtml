@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<a asp-action="DeleteFirstItem">İlk elemanı sil</a>
<form asp-action="Add" enctype="application/x-www-form-urlencoded" method="post">

    <input type="text" name="name" />
    <input type="number" name="score" />
    <button type="submit">Kaydet</button>
</form>


@foreach (var item in Model)
{
    <h2>
        @item
        <a asp-controller="SortedSetType" asp-action="DeleteItem" asp-route-name="@item">Sil</a>
    </h2>

}